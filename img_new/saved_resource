   <script>
       
    // JavaScript code to get and display the current date in Marathi
    var currentDateElement = document.getElementById('currentDate');
    
    // Create a new Date object
    var currentDate = new Date();
    
    // Format the date as per Marathi locale
    var options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
    var formattedDate = currentDate.toLocaleDateString('mr-IN', options); // 'mr-IN' is the code for Marathi (India)
    
    // Set the formatted date to the HTML element
    currentDateElement.textContent =  formattedDate;
  

   </script>
  <script>
      $(document).scroll(function() {
  var scroll = $(this).scrollTop();
  if (scroll >= 150) {
    $("#popUp").css("margin-left", "-425px");
    $("#plus").css("margin-left", "0px");
  }
});

$("#plus").click(function() {
  $("#popUp").css("margin-left", "0px");
  $("#plus").css("margin-left", "-425px");
});

$("#close").click(function() {
  $("#popUp").css("margin-left", "-425px");
  $("#plus").css("margin-left", "0px");
});
  </script>

